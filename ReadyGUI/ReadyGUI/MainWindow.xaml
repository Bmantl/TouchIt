<Window x:Class="ReadyGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ReadyGUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="1280"
        >
    <Window.Background>
        <ImageBrush/>
    </Window.Background>
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Images/readyBG.jpg"/>
        </Grid.Background>
        <Label x:Name="label" Content="Label" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="Get Into Position" HorizontalAlignment="Left" Margin="692,46,0,0" VerticalAlignment="Top" Width="355" Height="55" FontSize="40" FontWeight="Bold" FontStyle="Italic"/>
        <Image x:Name="key" HorizontalAlignment="Left" Height="82" Margin="978,129,0,0" VerticalAlignment="Top" Width="294" Source="Images/keysReg.png">
            <Image.OpacityMask>
                <ImageBrush ImageSource="Images/keysReg.png"/>
            </Image.OpacityMask>
        </Image>

    </Grid>
</Window>
